{
  "name": "plumber-saas",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:automation": "next lint --max-warnings=1000",
    "start": "next start",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "check-updates": "npx npm-check-updates",
    "check-security": "npm audit",
    "update-safe": "npx npm-check-updates -u --target patch",
    "update-minor": "npx npm-check-updates -u --target minor",
    "update-report": "npm run check-updates && npm run check-security",
    "test-after-update": "npm run lint:automation && npm run build",
    "update-manager": "node scripts/manage-updates.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
